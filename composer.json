{
    "name": "evrinoma/dto-common",
    "description": "Dto common",
    "keywords": [
        "Dto common"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolay Nikolaev",
            "email": "evrinoma@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "evrinoma/dto-bundle": "dev-master",
        "friendsofphp/php-cs-fixer": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Evrinoma\\DtoCommon\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "dto": {
            "type": "vcs",
            "url": "https://github.com/evrinoma/DtoBundle.git"
        }
    }
}
